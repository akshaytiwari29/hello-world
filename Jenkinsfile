pipeline {
          agent ('any') 
                  {
    
                    stages {
                            stage('dev') {
                                          steps {
                                                 echo "this is dev";
                                                 }
                                          }
                           stage('prod') {
                                           steps {
                                                   echo "this is prod";
                                                   }
                                          }
                             
                    }
          }
}
